<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <package name="default" namespace="/" extends="struts-default">

        <action name="selectAction"
                class="de.nordakademie.iaa.library.action.SelectAction" method="display">
            <result name="none">tiles/select.jsp</result>
        </action>

        <action name="resultAction" class="de.nordakademie.iaa.library.action.SelectAction">
            <result name="success">tiles/result.jsp</result>
        </action>
    </package>

</struts>